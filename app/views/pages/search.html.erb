  <div class="container">
    <input type="text" ng-model="query" placeholder="Search (e.g, Eminem)" />
  </div>
  <div class="right-col">
    <div class="video-list flush">
      <ul>
        <li class="fade in" ng-repeat="track in tracks" ng-click="createTrack(track)" collapse-on-click>
          <div>
          <a class="swipe">
            <div>
              <div class="crop">
                <img ng-src="{{track.thumbnail_url}}">
              </div>
              <div class="duration">{{track.duration | time}}</div>
            </div>
            <h3>{{track.title | truncate}}</h3>
            <p class="title">{{track.author}}</p>
            <p class="title"><span class="rating">{{track.rating}}%</span> {{track.view_count}} views</p>
          </a>
        </li>
      </ul>
    </div>
  </div>